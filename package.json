{
  "name": "insight",
  "version": "0.1.0",
  "description": "AI-powered legacy code documentation generator",
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "main": "./dist/cli/index.js",
  "bin": {
    "insight": "./dist/cli/index.js"
  },
  "scripts": {
    "dev": "tsx src/cli/index.ts",
    "build": "tsc",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint src/ --ext .ts",
    "format": "prettier --write src/",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist/",
    "prepublishOnly": "pnpm run clean && pnpm run build"
  },
  "keywords": [
    "documentation",
    "code-analysis",
    "ast",
    "ai",
    "claude",
    "typescript",
    "cli"
  ],
  "author": "Insight Team",
  "license": "MIT",
  "files": [
    "dist/",
    "templates/",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.3.1",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.0",
    "handlebars": "^4.7.8",
    "inquirer": "^9.2.0",
    "minimatch": "^10.0.3",
    "openai": "^4.28.0",
    "ora": "^7.0.1",
    "tree-sitter": "^0.20.6",
    "tree-sitter-python": "^0.20.4"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.9",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitest/ui": "^1.0.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}